{
  "name": "taro-react-boilerplate",
  "version": "0.0.4",
  "keywords": [
    "taro",
    "react",
    "typescript",
    "boilerplate"
  ],
  "description": "Taro React Boilerplate",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "none"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "deploy:h5": "gh-pages -d dist/h5",
    "commit": "git cz",
    "prepare": "husky install",
    "release": "release-it",
    "release:patch": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@antmjs/vantui": "^2.2.3",
    "@reduxjs/toolkit": "^1.8.6",
    "@tanstack/react-query": "^4.10.3",
    "@taroify/core": "0.1.0-alpha.1",
    "@taroify/icons": "0.1.0-alpha.1",
    "@tarojs/components": "3.5.7",
    "@tarojs/taro": "3.5.7",
    "ahooks": "^3.7.1",
    "date-fns": "^2.29.3",
    "jotai": "^1.8.5",
    "lodash-es": "^4.17.21",
    "ossaui": "^0.3.3",
    "react": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-query": "^3.39.2",
    "react-redux": "^8.0.4",
    "recoil": "^0.7.6",
    "zustand": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/runtime": "^7.19.4",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.8",
    "@release-it/conventional-changelog": "^5.1.1",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@tarojs/cli": "^3.5.7",
    "@tarojs/helper": "^3.5.7",
    "@tarojs/plugin-framework-react": "^3.5.7",
    "@tarojs/plugin-platform-alipay": "^3.5.7",
    "@tarojs/plugin-platform-jd": "^3.5.7",
    "@tarojs/plugin-platform-qq": "^3.5.7",
    "@tarojs/plugin-platform-swan": "^3.5.7",
    "@tarojs/plugin-platform-tt": "^3.5.7",
    "@tarojs/plugin-platform-weapp": "^3.5.7",
    "@tarojs/react": "^3.5.7",
    "@tarojs/router": "^3.5.7",
    "@tarojs/runtime": "^3.5.7",
    "@tarojs/shared": "^3.5.7",
    "@tarojs/taro-h5": "^3.5.7",
    "@tarojs/webpack5-runner": "^3.5.7",
    "@types/node": "^18.8.5",
    "@types/react": "^18.0.21",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "babel-plugin-import": "^1.13.5",
    "babel-preset-taro": "3.5.7",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "cz-customizable": "7.0.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-taro": "3.5.7",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.6",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-check-updates": "^16.3.11",
    "plop": "^3.1.1",
    "postcss": "^8.4.18",
    "postcss-rem-to-responsive-pixel": "^5.1.3",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "pretty-quick": "^3.1.3",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "release-it": "^15.5.0",
    "sass-loader": "13.1.0",
    "stylelint": "^14.13.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4",
    "weapp-tailwindcss-webpack-plugin": "^1.11.2",
    "webpack": "^5.74.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "author": {
    "name": "yangqingyun",
    "email": "yangqingyun@hotmai.com",
