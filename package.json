{
  "name": "taro-react-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "Taro React Boilerplate",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "none"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "commit": "git cz",
    "prepare": "husky install"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@tarojs/components": "3.4.9",
    "@tarojs/taro": "3.4.9",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/runtime": "^7.17.9",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@tarojs/cli": "^3.4.9",
    "@tarojs/mini-runner": "3.4.9",
    "@tarojs/plugin-framework-react": "^3.4.9",
    "@tarojs/react": "^3.4.9",
    "@tarojs/runtime": "^3.4.9",
    "@tarojs/webpack-runner": "3.4.9",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.16.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "autoprefixer": "^10.4.7",
    "babel-preset-taro": "3.4.9",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "cz-customizable": "6.3.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-taro": "3.4.9",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "npm-check-updates": "^12.5.11",
    "postcss": "^8.4.13",
    "postcss-rem-to-responsive-pixel": "^5.1.1",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "stylelint": "^14.8.2",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4",
    "weapp-tailwindcss-webpack-plugin": "^1.3.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
