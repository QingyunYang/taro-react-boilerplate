{
  "name": "taro-react-boilerplate",
  "version": "1.0.0",
  "keywords": [
    "taro",
    "react",
    "typescript",
    "boilerplate"
  ],
  "description": "Taro React Boilerplate",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "none"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "commit": "git cz",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.3",
    "@tarojs/components": "3.5.1",
    "@tarojs/helper": "3.5.1",
    "@tarojs/plugin-platform-alipay": "3.5.1",
    "@tarojs/plugin-platform-jd": "3.5.1",
    "@tarojs/plugin-platform-qq": "3.5.1",
    "@tarojs/plugin-platform-swan": "3.5.1",
    "@tarojs/plugin-platform-tt": "3.5.1",
    "@tarojs/plugin-platform-weapp": "3.5.1",
    "@tarojs/router": "3.5.1",
    "@tarojs/shared": "3.5.1",
    "@tarojs/taro": "3.5.1",
    "@tarojs/taro-h5": "3.5.1",
    "antd-mobile": "^5.13.1",
    "date-fns": "^2.29.1",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.0",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/runtime": "^7.17.9",
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "@tarojs/cli": "^3.5.1",
    "@tarojs/plugin-framework-react": "^3.5.1",
    "@tarojs/plugin-html": "^3.5.1",
    "@tarojs/react": "^3.5.1",
    "@tarojs/runtime": "^3.5.1",
    "@tarojs/webpack5-runner": "^3.5.1",
    "@types/react": "^18.0.15",
    "@types/webpack-env": "^1.16.4",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "babel-preset-taro": "3.5.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "cz-customizable": "6.3.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-taro": "3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.5.12",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.2",
    "npm-check-updates": "^15.3.3",
    "plop": "^3.1.1",
    "postcss": "^8.4.13",
    "postcss-rem-to-responsive-pixel": "^5.1.3",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "pretty-quick": "^3.1.3",
    "react-refresh": "^0.14.0",
    "stylelint": "^14.9.1",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.6.4",
    "weapp-tailwindcss-webpack-plugin": "^1.6.8",
    "webpack": "^5.73.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "author": {
    "name": "yangqingyun",
    "email": "yangqingyun@hotmai.com",
    "url": "https://github.com/QingyunYang"
  },
  "contributors": []
}
